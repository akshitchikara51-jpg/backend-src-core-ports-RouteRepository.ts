// @ts-nocheck
export interface Route {
  routeId: string;
  vesselType: string;
  fuelType: string;
  year: number;
  ghgIntensity: number;
  fuelConsumption: number;
  distance: number;
  totalEmissions: number;
  isBaseline: boolean; // Missing from required data structure initially
}

export interface RouteRepository {
  // Only the absolute minimum methods, incomplete
  findAllRoutes(): Promise<Route[]>;
  setBaseline(routeId: string): Promise<void>;
  // Missing methods for comparison, data manipulation, etc.
}
